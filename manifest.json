{
  "manifest_version": 3,
  "name": "Alaska Airlines EQM Estimator",
  "description": "Displays expected Alaska Airlines EQM earnings on partner websites.",
  "version": "1.0.1",
  "web_accessible_resources": [
    {
        "resources": ["scripts/airports.json"],
        "matches": [
            "https://www.aa.com/*",
            "https://aa.com/*",
            "https://www.britishairways.com/*",
            "https://www.finnair.com/*",
            "https://finnair.com/*"
        ]
    }
  ],
  "host_permissions": ["https://www.aa.com/*","https://aa.com/*","https://www.finnair.com/*","https://finnair.com/*","https://www.britishairways.com/*","https://britishairways.com/*"],
  "content_scripts": [
        {
            "js": ["scripts/jquery-3.6.4.min.js","scripts/americanair.js"],
            "matches": [
                "https://www.aa.com/booking/*",
                "https://aa.com/booking/*"
            ]
        },
        {
            "js": ["scripts/jquery-3.6.4.min.js","scripts/americanair2.js"],
            "matches": [
                "https://www.aa.com/booking/passengers*",
                "https://aa.com/booking/passengers*"
            ]
        },
        {
            "js": ["scripts/jquery-3.6.4.min.js","scripts/finnair.js"],
            "matches": [
                "https://www.finnair.com/us-en/*",
                "https://finnair.com/us-en/*"
            ]
        },
        {
            "js": ["scripts/jquery-3.6.4.min.js","scripts/ba.js"],
            "matches": [
                "https://www.britishairways.com/travel/book/*",
                "https://britishairways.com/travel/book/*"
            ]
        }
    ]
}